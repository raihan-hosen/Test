#include <stdio.h>

int main() {
    int N, X;
    scanf("%d", &N);

    for (int t = 0; t < N; t++) {
        scanf("%d", &X);
        int sum = 0;

        for (int i = 1; i <= X / 2; i++) {
            if (X % i == 0)
                sum += i;
        }

        if (sum == X)
            printf("%d eh perfeito\n", X);
        else
            printf("%d nao eh perfeito\n", X);
    }

    return 0;
}
