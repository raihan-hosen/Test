#include<stdio.h>
#include<math.h>
int main() {
    double a, b,c, temp1, temp2, temp3, temp4;
    scanf("%lf %lf %lf",&a,&b,&c);
    if(b>a){
        double temp=b;
        b=a;
        a=temp;
    }
    if(c>a){
        double temp=c;
        c=b;
        b=a;
        a=temp;
    }
    else if(c>b){   
        double temp=c;
        c=b;
        b=temp;
    }
    temp1= (a>=(b+c));
    temp2= (pow(a,2)==pow(b,2)+pow(c,2));
    temp3= (pow(a,2)>pow(b,2)+pow(c,2));
    temp4= (pow(a,2)<pow(b,2)+pow(c,2));
    if(temp1){
        printf("NAO FORMA TRIANGULO\n");
    }
    else{
        if(temp2){
            printf("TRIANGULO RETANGULO\n");
        }
        if(temp3){
            printf("TRIANGULO OBTUSANGULO\n");
        }
        if(temp4){
            printf("TRIANGULO ACUTANGULO\n");
        }
        if(a==b && b==c){
            printf("TRIANGULO EQUILATERO\n");
        }
        else if(a==b || b==c || a==c){
            printf("TRIANGULO ISOSCELES\n");
        }
    }
    return 0;
}
