#include <stdio.h>
int main() {
    int a, b, temp;
    while (1) {
        scanf("%d %d", &a, &b);
        if (a <= 0 || b <= 0) break;

        if (a > b) {
            temp = a;
            a = b;
            b = temp;
        }

        int sum = 0;
        for (int j = a; j <= b; j++) {
            printf("%d", j);       // print number
            sum += j;
            if (j < b) printf(" "); // print space only between numbers
        }
        printf(" Sum=%d\n", sum);
    }
    return 0;
}
