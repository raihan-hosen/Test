#include <stdio.h>

int main() {
    int N[20], temp;

    // Read 20 integers
    for (int i = 0; i < 20; i++) {
        scanf("%d", &N[i]);
    }

    // Swap elements symmetrically
    for (int i = 0; i < 10; i++) {
        temp = N[i];
        N[i] = N[19 - i];
        N[19 - i] = temp;
    }

    // Print the modified array
    for (int i = 0; i < 20; i++) {
        printf("N[%d] = %d\n", i, N[i]);
    }

    return 0;
}
